<form id="contact" action="registro2" method="post">
    <h3>Registro</h3>
    <h4>Paso 1:</h4>
    <fieldset>
      <input
        id="form-name"
        name="name"
        placeholder="Nombres"
        type="text"
        tabindex="1"
        required
        autofocus
      />
    </fieldset>
    <fieldset>
      <input
        id="form-lastname"
        name="lastname"
        placeholder="Apellidos"
        type="text"
        tabindex="2"
        required
      />
    </fieldset>
  
    <fieldset>
      <button
        name="submit"
        type="submit"
        id="contact-submit"
        data-submit="...Sending"
      >
        <a>Siguiente</a>
      </button>
    </fieldset>
  </form>
  